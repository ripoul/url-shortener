{
  "name": "@oddstronaut/gatsby-starter-tailwind",
  "description": "Shorten your url with many providers",
  "version": "1.0.0",
  "author": "@Ripoul & @VictorFAVREAU",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.20",
    "@fortawesome/free-solid-svg-icons": "^5.10.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "gatsby": "2.13.50",
    "gatsby-plugin-manifest": "2.2.4",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-postcss": "2.1.2",
    "gatsby-plugin-purgecss": "4.0.0",
    "gatsby-plugin-react-helmet": "3.1.2",
    "graphql": "14.4.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-toastify": "^5.3.2"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint src",
    "fixlint": "eslint src --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "10.0.2",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "1.18.2",
    "tailwindcss": "1.0.6",
    "typescript": "3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ripoul/url-shortener"
  },
  "bugs": {
    "url": "https://github.com/ripoul/url-shortener/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
